<configuration>
  <connectionStrings>
    <add name="jian" connectionString="Data Source=.;Initial Catalog=MyPhotos;integrated security=true"/>
  </connectionStrings>
  <system.web>
    <compilation debug="true" targetFramework="4.0"/>
  </system.web>


  <system.webServer>
    <handlers>
      <add name="wm" verb="*" path="images/*.jpg" type="WaterMaker"/>
    </handlers>
  </system.webServer>
</configuration>